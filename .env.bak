# Phase 1 Environment Configuration
# Copy this file to .env and update the values

# Database Configuration
POSTGRES_DB=mlops_platform
POSTGRES_USER=postgres
POSTGRES_PASSWORD=password

# Redis Configuration
REDIS_HOST=redis
REDIS_PORT=6379

# MinIO Configuration
MINIO_ROOT_USER=minioadmin
MINIO_ROOT_PASSWORD=minioadmin
MINIO_ENDPOINT=http://minio:9000

# MLflow Configuration
MLFLOW_BACKEND_STORE_URI=postgresql://postgres:password@postgres:5432/mlflow
MLFLOW_DEFAULT_ARTIFACT_ROOT=s3://mlflow-artifacts
MLFLOW_S3_ENDPOINT_URL=http://minio:9000

# API Gateway Configuration
API_GATEWAY_PORT=8080
OAUTH_TOKEN=mock_token

# ModelCatalogue Configuration
MODELCATALOGUE_DB_URI=postgresql://postgres:password@postgres:5432/modelcatalogue

# Jupyter Configuration
JUPYTER_TOKEN=
JUPYTER_PASSWORD=

# Git Information (for lineage tracking)
GIT_REPO_URL=https://github.com/your-org/your-repo
GIT_COMMIT_SHA=your_commit_sha
